<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyName>Taurus.Core</AssemblyName>
    <RootNamespace>Taurus.Core</RootNamespace>
    <Description>Taurus.mvc is a simple mvc or webapi framework for asp.net core</Description>
    <Company>路过秋天 （2018-04-17）</Company>
    <Authors>路过秋天</Authors>
    <Copyright>Copyright ©  路过秋天 2016-2096</Copyright>
    <Version>2.2.3.5</Version>
    <Product>Taurus.Core .NET Core 版本</Product>
    <PackageProjectUrl>http://taurus.cyqdata.com</PackageProjectUrl>
    <PackageId>Taurus.MVC.Core</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageReleaseNotes>Taurus.mvc is a simple mvc or webapi framework for asp.net core</PackageReleaseNotes>
    <PackageTags>Taurus.MVC Core ASP.NET aspnetcore webapi</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netcoreapp2.0\Taurus.Core.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Properties\AssemblyInfo.cs" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="cyqdatacore" Version="5.7.8.6" />
    <PackageReference Include="Microsoft.AspNetCore.All" Version="2.0.8" />
  </ItemGroup>

</Project>
